  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vladmandic/face-api/issues"
  },
  "homepage": "https://github.com/vladmandic/face-api#readme",
  "dependencies": {
    "@vladmandic/pilogger": "^0.2.14"
  },
  "devDependencies": {
    "@tensorflow/tfjs": "^3.0.0",
    "@tensorflow/tfjs-backend-wasm": "^3.0.0",
    "@tensorflow/tfjs-node": "^3.0.0",
    "@tensorflow/tfjs-node-gpu": "^3.0.0",
    "@types/node": "^14.14.25",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "chokidar": "^3.5.1",
    "esbuild": "^0.8.42",
    "eslint": "^7.19.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "rimraf": "^3.0.2",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  }
